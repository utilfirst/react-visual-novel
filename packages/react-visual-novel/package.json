{
  "name": "react-visual-novel",
  "version": "0.2.2",
  "description": "React library for building web-based visual novels",
  "repository": "https://github.com/utility-first/react-visual-novel",
  "license": "MIT",
  "author": "Ayan Yenbekbay <ayan@utility-first.co> (https://utility-first.co)",
  "sideEffects": false,
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && run-s --silent build:*",
    "build:css": "pnpm run generate:css --minify",
    "build:ts": "tsup",
    "dev": "run-p --silent dev:*",
    "dev:css": "pnpm run generate:css --watch",
    "dev:ts": "tsup --watch",
    "generate:css": "tailwindcss -i index.css -o dist/index.css",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@radix-ui/react-popover": "1.0.2",
    "@radix-ui/react-use-rect": "1.0.0",
    "@react-hookz/web": "16.0.0",
    "@supercharge/promise-pool": "2.3.2",
    "async-preloader": "7.0.0",
    "framer-motion": "7.6.1",
    "intrinsic-scale": "3.0.4",
    "mdast-util-from-markdown": "1.2.0",
    "micro-observables": "1.7.2",
    "moize": "6.1.3",
    "phosphor-react": "1.4.1",
    "react-is": "18.2.0",
    "string-dedent": "3.0.1",
    "tailwind-merge": "1.6.2",
    "use-event-callback": "0.1.0",
    "use-long-press": "2.0.2"
  },
  "devDependencies": {
    "@tsconfig/strictest": "1.0.2",
    "@types/howler": "2.2.7",
    "@types/intrinsic-scale": "3.0.0",
    "@types/mdast": "3.0.10",
    "@types/react": "18.0.21",
    "@types/react-is": "17.0.3",
    "autoprefixer": "*",
    "csstype": "*",
    "daisyui": "2.31.0",
    "howler": "2.2.3",
    "npm-run-all": "4.1.5",
    "postcss": "*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "tailwindcss": "3.1.8",
    "tailwindcss-scrims": "1.0.0",
    "tsup": "6.3.0",
    "use-query-params": "2.1.1"
  },
  "peerDependencies": {
    "howler": "^2.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "use-query-params": "^2.1.0"
  }
}
